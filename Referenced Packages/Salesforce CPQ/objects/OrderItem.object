        <fullName>ContractingMethod__c</fullName>
        <deprecated>false</deprecated>
        <description>Controls the contracting behavior of an Order Product. &quot;Inherit&quot; uses the behavior of the parent Order. &quot;Contract Separately&quot; generates a separate Contract for
            the Order Product. &quot;No Contract&quot; skips the Order Product altogether during the contracting process.</description>
        <externalId>false</externalId>
        <inlineHelpText>Controls the contracting behavior of an Order Product.</inlineHelpText>
        <label>Contracting Method</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Inherit</fullName>
                    <default>true</default>
                </value>
                <value>
                    <fullName>Contract Separately</fullName>
                    <default>false</default>
                </value>
                <value>
                    <fullName>Do Not Contract</fullName>
                    <default>false</default>
                </value>
            </valueSetDefinition>
        </valueSet>
